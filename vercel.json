{
    "version": 2,
    "cleanUrls": true,
    "redirects": [
        {
            "source": "/",
            "destination": "/dashboard/",
            "permanent": false
        }
    ],
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/$1.js"
        },
        {
            "source": "/dashboard",
            "destination": "/dashboard/index.html"
        },
        {
            "source": "/dashboard/",
            "destination": "/dashboard/index.html"
        }
    ]
}